<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <machpay>
                <active>1</active>
                <model>Machpay</model>
                <order_status>pending</order_status>
                <status_approved>processing</status_approved>
                <status_canceled>canceled</status_canceled>
                <title>MachPay</title>
                <refund_available>0</refund_available>
                <can_authorize>0</can_authorize>
                <can_capture>0</can_capture>
                <can_void>0</can_void>
                <can_use_checkout>1</can_use_checkout>
                <is_gateway>0</is_gateway>
                <sort_order>0</sort_order>
                <credentials>
                    <endpoint>https://biz-sandbox.soymach.com</endpoint>
                    <token backend_model="Magento\Config\Model\Config\Backend\Encrypted"/>
                </credentials>
                <cancel_orders>
                    <active>1</active>
                    <schedule>F</schedule>
                    <time>00,05,00</time>
                    <timeinterval>15</timeinterval>
                </cancel_orders>
            </machpay>
        </payment>
    </default>
</config>
